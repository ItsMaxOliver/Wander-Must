<body class="profile-page sidebar-collapse">
  <nav class="navbar navbar-transparent navbar-color-on-scroll fixed-top navbar-expand-lg" color-on-scroll="100" id="sectionsNav">
    <div class="container">
      <div class="navbar-translate">
        <a class="navbar-brand" href="index">
          Wander-Must </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
          <span class="navbar-toggler-icon"></span>
          <span class="navbar-toggler-icon"></span>
        </button>
      </div>
      <div class="collapse navbar-collapse">
        <ul class="navbar-nav ml-auto">

          <li class="nav-item dropdown">
            <a href="#" class="dropdown-toggle nav-link" data-toggle="dropdown">
              <i class="fa fa-user-circle" title="Profile Page"> </i>
            </a>
            
            <div class="dropdown-menu dropdown-with-icons">

              <form id="signin" name="signin" method="post" action="signin">
                <label for="email">Email Address</label>
                <input class="text" name="email" type="text" />
                <label for="password">Password</label>
                <input name="password" type="password" />
                <input class="btn" type="submit" value="Sign In" />
              </form>

            </div>
          
          </li>

          <li class="nav-item">
            <span class="suitcase-input" data-toggle="modal" data-target="#suitcase-modal">
              <a class="nav-link" data-toggle="tooltip" title="Add new Suitcase" data-placement="middle" data-original-title="Add new suitcase">
                <i class="fa fa-suitcase" data-toggle="tooltip" title="Add new Suitcase"> </i>
              </a>
            </span>
          </li>
        </ul>
      </div>
    </div>
  </nav>


  {{{ body }}}

  <!-- Add New Suitcase Modal -->
  <div id="suitcase-modal" class="modal fade" role="dialog">
    <div class="modal-dialog modal-lg">

      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-header text-center">
          <h2 class="modal-title">
            <strong>Add a new suitcase</strong>
          </h2>
          <button type="button" class="close" data-dismiss="modal">&times;</button>
        </div>
        <div class="modal-body text-center">
          <div class="md-form mb-5">
            <i class="fa fa-map-marker prefix grey-text"></i>
            <input type="text" id="autocomplete" class="form-control validate">
            <label data-error="wrong" data-success="right" for="autocomplete">City</label>
          </div>

          <div class="row">
            <div class="col-6">
              <div class="md-form mb-5">
                <div class='input-group date' id='datetimepicker1'>
                  <input type='text' class="form-control" />
                  <span class="input-group-addon">
                    <span class="fa fa-calendar"></span>
                  </span>
                </div>
                <label data-error="wrong" data-success="right" for="form29">Start Date</label>
              </div>
            </div>
            <div class="col-6">
              <div class="md-form mb-5">
                <div class='input-group date' id='datetimepicker2'>
                  <input type='text' class="form-control" />
                  <span class="input-group-addon">
                    <span class="fa fa-calendar"></span>
                  </span>
                </div>
                <label data-error="wrong" data-success="right" for="form29">End Date</label>
              </div>
            </div>
          </div>


          <div class="md-form mb-5">
            <i class="fa fa-plane prefix grey-text"></i>
            <select class="custom-select" id="travelselect">
              <option selected>&nbsp;</option>
              <option value="1">Vacation</option>
              <option value="2">Adventure</option>
              <option value="3">Luxury</option>
            </select>
            <label data-error="wrong" data-success="right" for="travelselect">Travel Type</label>
          </div>

          <div class="md-form">
            <i class="fa fa-pencil prefix grey-text"></i>
            <textarea type="text" id="form8" class="md-textarea form-control" rows="4"></textarea>
            <label data-error="wrong" data-success="right" for="form8">Notes</label>
          </div>

        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default">Start Packing!</button>
      </div>
    </div>

  </div>

  <!-- Suitcase Items Modal -->
  <div class="modal fade" id="opencase-modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">USERNAME 's CITY suitcase</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <div id="items">
            <!-- Toiletries -->
            <div class="row">
              <div class="offset-1 col-11">
                <div class="title row">
                  <span class="badge badge-pill badge-info">Toiletries</span>
                </div>
                <div class="row" id="toiletries">
                  <div class="form-check offset-1 col-5">
                    <label class="form-check-label">
                      <input class="form-check-input" type="checkbox" value=""checked> Tootbrush
                      <span class="form-check-sign">
                        <span class="check"></span>
                      </span>
                    </label>
                  </div>
                  <div class="form-check offset-1 col-5">
                    <label class="form-check-label">
                      <input class="form-check-input" type="checkbox" value=""checked> Tootpaste
                      <span class="form-check-sign">
                        <span class="check"></span>
                      </span>
                    </label>
                  </div>
                </div>
              </div>
            </div>
            <!-- Clothing -->
            <div class="row">
              <div class="offset-1 col-11">
                <div class="title row">
                  <span class="badge badge-pill badge-primary">Clothing</span>
                </div>
                <div class="row" id="clothing">
                    <div class="form-check offset-1 col-5">
                    <label class="form-check-label">
                      <input class="form-check-input" type="checkbox" value=""checked> Pajamas
                      <span class="form-check-sign">
                        <span class="check"></span>
                      </span>
                    </label>
                  </div>
                  <div class="form-check offset-1 col-5">
                    <label class="form-check-label">
                      <input class="form-check-input" type="checkbox" value=""checked> T-Shirt
                      <span class="form-check-sign">
                        <span class="check"></span>
                      </span>
                    </label>
                  </div>
                </div>
              </div>
            </div>

            <!-- Accessories -->            
            <div class="row">
              <div class="offset-1 col-11">
                <div class="title row">
                  <span class="badge badge-pill badge-default">Accessories</span>
                </div>
                <div class="row" id="accessories">
                  <div class="form-check offset-1 col-5">
                    <label class="form-check-label">
                      <input class="form-check-input" type="checkbox" value=""checked> Hat
                      <span class="form-check-sign">
                        <span class="check"></span>
                      </span>
                    </label>
                  </div>
                  <div class="form-check offset-1 col-5">
                    <label class="form-check-label">
                      <input class="form-check-input" type="checkbox" value=""checked> Scarf
                      <span class="form-check-sign">
                        <span class="check"></span>
                      </span>
                    </label>
                  </div>
                </div>
              </div>
            </div>

          </div>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-secondary btn-round" data-dismiss="modal" src="assets/suitcase/:id">Open Suitcase</button>
            <button type="button" class="btn btn-primary btn-round">Add Selected Items</button>
        </div>
      </div>
    </div>
  </div>



